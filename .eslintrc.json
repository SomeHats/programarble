{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "new-cap": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "prefer-rest-params": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0
  }
}
